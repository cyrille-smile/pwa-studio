{
    "version": 2,
    "name": "venia",
    "alias": [
        "magento-venia-develop"
    ],
    "scope": "magento-research",
    "env": {
        "UPWARD_JS_BIND_LOCAL": "false",
        "NOW_DEBUG": "1",
        "DEBUG": "express-sharp",
        "SHARP_IGNORE_GLOBAL_LIBVIPS": "1"
    },
    "builds": [
        {
            "config": {
                "includeFiles": [
                    ".env",
                    "venia-upward.yml",
                    "static/*",
                    "templates/*",
                    "dist/*",
                    "dist/**/*"
                ],
                "maxLambdaSize": "50mb"
            },
            "src": "packages/venia-concept/server-lambda.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "^/([0-9A-Fa-e]\\.svg)",
            "dest": "packages/venia-concept/$1"
        },
        {
            "src": "^/(precache-manifest.*\\.js)",
            "dest": "packages/venia-concept/$1"
        },
        {
            "src": "^/(sw\\.js)",
            "dest": "packages/venia-concept/$1"
        },
        {
            "src": "^/(js/.*\\.js)",
            "dest": "packages/venia-concept/$1"
        },
        {
            "src": "/(.*)",
            "dest": "packages/venia-concept/server-lambda.js"
        }
    ],
    "regions": [
        "sfo1",
        "bru"
    ],
    "public": true
}
