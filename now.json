{
    "name": "venia",
    "alias": [
        "magento-venia-develop"
    ],
    "scope": "magento-research",
    "env": {
        "UPWARD_JS_BIND_LOCAL": "false"
    },
    "builds": [
        {
            "config": {
                "maxLambdaSize": "50mb"
            },
            "src": "packages/venia-concept/server.js",
            "use": "@now/node"
        },
        {
            "src": "packages/venia-concept/@(.env.dist,static/**/*,templates/*,server.js,venia-upward.yml,validate-environment.js)",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/*",
            "dest": "packages/venia-concept/server.js"
        }
    ],
    "regions": [
        "sfo1",
        "bru"
    ],
    "public": true
}
